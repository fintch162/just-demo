<% if @token_expire %>
	location.reload();
<% else %>
	<% if @error %>
	$('[id='+<%= @student.id %>+']').html('Error occured');
	<% flash[:alert] = nil %>
	<% else %>
	$('[id='+<%= @student.id %>+']').html('Synchronized');
	$('#remove'+<%= @student.id %>).html("<%= escape_javascript(button_to 'Remove', instructor_remove_synchronized_contacts_path(id: @student.id),remote: true, data: {confirm: 'Are you sure?', disable_with: "Removing..."}, class: 'btn-link') %>");
	<% end %>
<% end %>