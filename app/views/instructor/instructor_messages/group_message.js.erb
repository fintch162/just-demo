<% if @error.blank? %>
  console.log("<%= @std_name_list.count %>");
  $("#successMsg").text("SMS sent to <%= @sutdent_ids.count %> students").show().delay(3000)
                  .queue(function() {
                      $(this).fadeOut('slow');
                      $("#send_msg").prop("disabled", false);
                      $(this).dequeue();
                  });
<% else %>
    $("#error_msg").text("<%= @error %>").css("font-size","13px").show().delay(3000)
                  .queue(function() {
                      $(this).fadeOut('slow');
                      $("#send_msg").prop("disabled", false);
                      $(this).dequeue();
                  });
<% end %>
$("#send_msg").val("Send");

