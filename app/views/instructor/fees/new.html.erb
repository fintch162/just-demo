<%= title "Add Fee" %>
<% if !admin_user_signed_in? %>
  <%= render "instructor/shared/login" %>
<% else %>
  <div class="main">
    <div class="container">
      <ul class="breadcrumb">
        <li><a href="/instructor">Home</a></li>
        <li><a href="/instructor/instructor_students">Students</a></li>
        <li class="active"><%= @instructor_student.student_name %></li>
      </ul>
      <div class="row margin-bottom-40">
        <div class="col-md-9 col-sm-9">
          <div class="row">
            <h1>Add Fees </h1>
            <%= render "form" %>
          </div>
        </div>
      </div>
    </div>
  </div>
<% end %>
