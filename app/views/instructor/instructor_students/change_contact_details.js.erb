<% @primary_contact = @instructor_student.student_contacts.where(:primary_contact => true) %>
<% @contact = @instructor_student.student_contacts.where(:primary_contact => false) %>
 var content = ""; 
  content =  '<div class="col-sm-8 col-xs-12 padding-top-10 ">' +
               '<div class="row">' + 
              '<% if @primary_contact.length > 0 %><% @primary_contact.each do |contact| %><div class="col-xs-4 text-right text-muted"><%= !contact.relationship.blank? ? contact.relationship : "Contact" %></div><div class="col-xs-8"><%= contact.contact %><i class="fa fa-star"></i></div><% end %><% end %>' +
               '<% if @contact.length > 0 %><% @contact.each do |contact| %><div class="col-xs-4 text-right text-muted"><%= !contact.relationship.blank? ? contact.relationship : "Contact" %></div><div class="col-xs-8"><%= contact.contact %></div><% end %><% end %>' +
               '<% if @contact.length == 0  &&  @primary_contact.length == 0%><div class="col-xs-4 text-right text-muted">Contact</div><div class="col-xs-8 text-warning"><i class="fa fa-warning"></i> Data needed </div><% end %>' +
              '</div>' + 
              '</div>';
              
 

 $(".contact_details").html(content);
 $(".edit_contact_details").hide();
 $(".contact_details").show();
 $("#deleted_contacts").val("");
 window.location.reload();