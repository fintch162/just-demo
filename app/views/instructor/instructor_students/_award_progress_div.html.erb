<input type="hidden" name="deleted_awards" id="deleted_awards"></input>
<div class="col-sm-12 col-xs-12 padding-top-10">
  <div class="row">
    <div class="col-xs-6 text-right text-muted padding-top-10"> 
      <h3>Award Details</h3>
    </div>
  </div>

  <% @rem_array = [] %>
  <% Award.all.order(:position).each do |a|%>
    <% @cnt = 0 %>
      <div class="row row_margin <%=a.id %>" id="<%=a.id %>"> 
        <%=render 'student_award_progress',:a=>a %>
      </div>
  <%end %>
</div>