                          <% end %>
                          <%# @fee.each do |fee| %>
                            <%# if !fee.amount.blank? %>
                              <!-- <strong>$<%#= fee.amount %></strong>  -->
                            <%# end %>
                            <%# if !fee.course_start.blank?  &&  !fee.course_end.blank? %>
                              <!-- <small><%#= fee.course_start.strftime("%e %b %Y") + ' - ' +  fee.course_end.strftime("%e %b %Y") %></small><br/> -->
                            <%# end %>
                          <%# end %>
                        <% end %>
