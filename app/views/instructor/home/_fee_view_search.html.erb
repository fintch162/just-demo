                      <th class="Course-fee" style="white-space:nowrap; <%= current_admin_user.more_or_less_months.count == 0 ? "font-size: 16px;" : "font-size: #{current_admin_user.more_or_less_months.first.font_size*0.8}px" %>">Course fee</th>
                      <th></th>
                      <% count = 0 %>
                    </tr>
                  </thead> 
                  <tbody>
                    <% group.instructor_students.each do |student| %>
                      <% @instructor_student_award = InstructorStudentAward.where(:instructor_student_id => student.id)#.where("award_progress NOT IN (?)", @award_progress) %>
