                </div>
              </div>
              <% show_cnt = 1 %>
              <table class="table table-hover sample_editable_2"  style="<%= current_admin_user.more_or_less_months.count == 0 ? "font-size: 16px;" : "font-size: #{current_admin_user.more_or_less_months.first.font_size}+ 2 px" %>" >
                <thead>
                  <tr>
                    <%= render "common_table_header_fee_view_group_class" %>

                    
