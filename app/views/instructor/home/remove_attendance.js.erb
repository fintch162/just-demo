var replaceDiv = "";
replaceDiv = $("#atteS-<%= @attendance.attendance_date %>-<%= @attendance.instructor_student_id %>");
replaceDiv.attr('href', '<%= instructor_mark_present_path(@attendance.group_class_id, @attendance.instructor_student_id, @attendance.attendance_date,date: params[:date]) %>');


replaceDiv.removeClass('green').addClass('grey');
replaceDiv.find('i').removeClass('fa-check fa-times').addClass('fa-minus');


