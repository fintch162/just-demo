var next_page_message = []
<% @messages["data"].each do |message| %>
  next_page_message.push('<tr class="unread" data-messageid="1"><td class="inbox-small-cells"><div class="checker"><span><input class="mail-checkbox" type="checkbox"></input></span></div></td><td class="view-message hidden-xs"><%= link_to message["name"], manage_conversastions_messages_path(message["phone_number"]) %></td><td class="view-message"></td><td class="view-message inbox-small-cells">  <button class="btn red btn-xs" type="button"><%= message["message_count"] %></button></td><td class="view-message text-right"><% seconds = message["last_message_time"] / 1000 if !message["last_message_time"].nil? %><%= Time.at(seconds).strftime("%H:%M %P") if !message["last_message_time"].nil? %></td></tr>');
<% end %>
$("tbody").html(next_page_message)
