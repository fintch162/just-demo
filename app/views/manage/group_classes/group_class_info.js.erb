<% if params[:from_booking].present? %>
	$('.gc-info-<%= @group_class.venue_id.to_s + @group_class.day.to_s %>').html('<%= escape_javascript(render partial: "gc_info_booking_schedule") %>');
<% else %>
	$('.gc-info-<%= @group_class.venue_id.to_s + @group_class.day.to_s %>').html('<%= escape_javascript(render partial: "group_class_info") %>');
	<% if params[:job_id].present? %>
		$('html, body').animate({
	        scrollTop: $('.gc-info-<%= @group_class.venue_id.to_s + @group_class.day.to_s %>').offset().top - 100
	    }, 1500);
	<% end %>
<% end %>