$('#gc-'+<%= @group_class.id.to_s %>).removeClass('blue');
$('#gc-'+<%= @group_class.id.to_s %>).removeClass('red');
$('#gc-'+<%= @group_class.id.to_s %>).removeClass('green');
<% if params[:booking_status] == 'true' %>
	$('#gc-'+<%= @group_class.id.to_s %>).addClass('green');
<% elsif params[:booking_status] == nil %>
	$('#gc-'+<%= @group_class.id.to_s %>).addClass('blue');
<% else %>
	$('#gc-'+<%= @group_class.id.to_s %>).addClass('red');
<% end %>
$('#gc-'+<%= @group_class.id.to_s %>).html(
"<%= @group_class.time.strftime('%I:%M %P') %>");

$('.gc-info-<%= @group_class.venue_id.to_s + @group_class.day.to_s %>').html('<%= escape_javascript(render partial: "group_class_info") %>');