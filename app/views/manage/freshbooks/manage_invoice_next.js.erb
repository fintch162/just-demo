var next_page_message = []
<% @inv_retnn.each do |inv| %>
  next_page_message.push('<tr class="odd gradeX"><td><%= link_to inv["number"] , inv["links"]["view"], :target => "_blank"%></td><td><%= inv["organization"] %></td><td><%= inv["updated"].to_date.strftime("%d/%m/%Y") %></td><td><%= inv["amount"] %>   </td><td><%= inv["status"] %>  </td><td><%= link_to "REF #{inv["po_number"]}",manage_job_path(inv["po_number"]) %></td><td><% @m = Job.all.pluck(:id) %>                    <% if @m.include?(inv["po_number"].to_i)%>                     <%= Job.find(inv["po_number"]).job_status %>                  <%end %> </td><td>                    <% @m = Job.all.pluck(:id) %>                    <% if @m.include?(inv["po_number"].to_i)%><% if !Job.find(inv["po_number"]).fee_total.nil? %>                        $ <%= Job.find(inv["po_number"]).fee_total %>                      <% end %>                    <%end %> </td><td>                    <% @m = Job.all.pluck(:id) %> <% if @m.include?(inv["po_number"].to_i)%>                      <% if  !Job.find(inv["po_number"]).referral.nil? %>                        $ <%= Job.find(inv["po_number"]).referral %>                      <% end %>                    <%end %></td></tr>');
<% end %>
$("tbody").html(next_page_message)